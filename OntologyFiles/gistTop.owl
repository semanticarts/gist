		<rdfs:label rdf:datatype="&xsd;string">identifies</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">The thing to which the identifier refers.</rdfs:comment>
		<skos:prefLabel rdf:datatype="&xsd;string">identifies</skos:prefLabel>
	</rdf:Description>
	
