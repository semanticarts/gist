					<rdf:Description rdf:about="&gist;Person">
					</rdf:Description>
					<rdf:Description rdf:about="&gist;Template">
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</rdfs:domain>
		<rdfs:range>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
