		<skos:prefLabel rdf:datatype="&xsd;string">end</skos:prefLabel>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&gist;epoch">
		<rdfs:label rdf:datatype="&xsd;string">epoch</rdfs:label>
