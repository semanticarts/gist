		<skos:prefLabel rdf:datatype="&xsd;string">epoch</skos:prefLabel>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&gist;governedBy">
		<rdfs:label rdf:datatype="&xsd;string">governed by</rdfs:label>
