CONSTRUCT {
    ?report a sh:ValidationReport ;
        sh:conforms false ;
        sh:result
            [
                a sh:ValidationResult ;
                sh:focusNode ?g ;
                sh:resultMessage ?error ;
                sh:resultSeverity sh:Warning ;
                sh:sourceConstraintComponent <urn:constraint:renamed-entity>
            ] .
}
WHERE {
    GRAPH ?g {
        ?s ?p ?o 
        FILTER(STRSTARTS(STR(?g), "https://ontologies.semanticarts.com/gist/"))
    }
    bind(<urn:renamed-entity-validation-report> as ?report)
    bind(CONCAT("WARNING: incorrect namespace: https://ontologies.semanticarts.com/gist/",
        REPLACE(STR(?g), '^.*[/#]', ''), ". This named graph should now be: https://w3id.org/semanticarts/gist/", REPLACE(STR(?g), '^.*[/#]', '')
        ) as ?error)
}
