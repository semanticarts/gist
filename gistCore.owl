		<rdfs:comment rdf:datatype="&xsd;string">Relates a RatioUnit such as meters/second to the denominator Unit (e.g. second).</rdfs:comment>
		<rdfs:domain rdf:resource="&gist;RatioUnit"/>
		<rdfs:range rdf:resource="&gist;UnitOfMeasure"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&gist;describedIn">
