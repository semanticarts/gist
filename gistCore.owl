	<owl:ObjectProperty rdf:about="&gist;connectedTo">
		<rdfs:label rdf:datatype="&xsd;string">Connected To</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">A non-owning, non-causal, non-subordinate (i.e., peer-to-peer) relationship.</rdfs:comment>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&gist;containedText">
		<rdfs:label rdf:datatype="&xsd;string">Contained Text</rdfs:label>
