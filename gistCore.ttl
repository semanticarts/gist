						gist:Category
						[
							a owl:Class ;
							owl:unionOf (
								[
									a owl:Restriction ;
									owl:onProperty gist:hasSubCategory ;
									owl:someValuesFrom gist:Category ;
								]
								[
									a owl:Restriction ;
									owl:onProperty gist:hasSuperCategory ;
									owl:someValuesFrom gist:Category ;
								]
