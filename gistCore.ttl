	skos:example "Mobile, fax, or landline phone number."^^xsd:string ;
	skos:prefLabel "Telephone Number"^^xsd:string ;
	.

gist:Temperature
	a owl:Class ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			gist:Magnitude
			[
				a owl:Restriction ;
				owl:onProperty gist:hasUnitOfMeasure ;
				owl:someValuesFrom gist:TemperatureUnit ;
			]
		) ;
	] ;
	skos:definition "The degree or intensity of heat present in a substance or object, especially as expressed according to a comparative scale."^^xsd:string ;
	skos:prefLabel "Temperature"^^xsd:string ;
