	skos:prefLabel "Text"^^xsd:string ;
	.

gist:TimeInstant
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty gist:epoch ;
			owl:someValuesFrom xsd:double ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gist:hasPrecision ;
			owl:someValuesFrom gist:Duration ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gist:localDate ;
			owl:someValuesFrom xsd:string ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gist:localDateTime ;
			owl:someValuesFrom xsd:dateTime ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gist:localTime ;
			owl:someValuesFrom xsd:string ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gist:universalDate ;
			owl:someValuesFrom xsd:string ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gist:universalDateTime ;
			owl:someValuesFrom xsd:dateTime ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gist:universalTime ;
			owl:someValuesFrom xsd:string ;
		]
		;
	owl:disjointWith gist:UnitOfMeasure ;
	skos:definition "All time instants are Greenwich and the local equal universal."^^xsd:string ;
	skos:example "A literal instant (as in 12:01.0001 January 1, 2008), or a broader but still single point in time (January 1, 2008). In the latter case, we are declaring a time instant to be an interval with no duration (or really a duration only equal to its precision)."^^xsd:string ;
	skos:prefLabel "Time Instant"^^xsd:string ;
	skos:scopeNote "Our identity criteria require that something refers to each Time Instant instance."^^xsd:string ;
	.

gist:TimeZone
	a owl:Class ;
	owl:equivalentClass [
