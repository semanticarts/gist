@prefix : <https://ontologies.semanticarts.com/o/gistCore#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gist: <https://ontologies.semanticarts.com/gist/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@base <https://ontologies.semanticarts.com/o/gistCore> .

<https://ontologies.semanticarts.com/o/gistCore> rdf:type owl:Ontology ;
                                                  owl:versionIRI <https://ontologies.semanticarts.com/o/gistCoreX.x.x> ;
                                                  skos:definition "gist is a minimalist upper ontology created by Semantic Arts"^^xsd:string ;
                                                  gist:license "https://creativecommons.org/licenses/by-sa/3.0/"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
skos:note rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#scopeNote
skos:scopeNote rdf:type owl:AnnotationProperty .


###  https://ontologies.semanticarts.com/gist/domainIncludes
gist:domainIncludes rdf:type owl:AnnotationProperty ;
                    skos:definition "Relates a property to a class that is (one of) the type(s) the property is expected to be used on."^^xsd:string ;
                    skos:example "The domain for the property gist:hasMember includes gist:Collection."^^xsd:string ;
                    skos:prefLabel "domain includes"^^xsd:string ;
                    skos:scopeNote "This property is used to guide the ontology user; like all annotation properties, it does not play a role in inference."^^xsd:string ,
                                   "This property means the same thing as the Schema.org version, which is an rdf:Property and not compliant with OWL-DL."^^xsd:string ;
                    rdfs:subPropertyOf skos:scopeNote .


###  https://ontologies.semanticarts.com/gist/license
gist:license rdf:type owl:AnnotationProperty ;
             skos:definition "An annotation for providing the licensing on this or derivative ontologies"^^xsd:string ;
             skos:prefLabel "license"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/rangeIncludes
gist:rangeIncludes rdf:type owl:AnnotationProperty ;
                   skos:definition "Relates a property to a class that constitutes (one of) the expected type(s) for values of the property."^^xsd:string ;
                   skos:example "The range for the property gist:isCategorizedBy includes gist:Category."^^xsd:string ;
                   skos:prefLabel "range includes"^^xsd:string ;
                   skos:scopeNote "This property is used to guide the ontology user; like all annotation properties, it does not play a role in inference."^^xsd:string ,
                                  "This property means the same thing as the Schema.org version, which is an rdf:Property and not compliant with OWL-DL."^^xsd:string ;
                   rdfs:subPropertyOf skos:scopeNote .


#################################################################
#    Object Properties
#################################################################

###  https://ontologies.semanticarts.com/gist/accepts
gist:accepts rdf:type owl:ObjectProperty ;
             skos:definition "The types of input messages that will be allowed"^^xsd:string ;
             skos:prefLabel "accepts"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/affects
gist:affects rdf:type owl:ObjectProperty ;
             owl:inverseOf gist:isAffectedBy ;
             skos:definition "The subject has or had or will have an effect on the object"^^xsd:string ;
             skos:prefLabel "affects"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/allows
gist:allows rdf:type owl:ObjectProperty ;
            rdfs:domain gist:Intention ;
            rdfs:range gist:Behavior ;
            skos:definition "The intention (say a grant) allows a particular kind of activity (for instance egress)"^^xsd:string ;
            skos:prefLabel "allows"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/comesFromAgent
gist:comesFromAgent rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf gist:hasParticipant ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( gist:Organization
                                               gist:Person
                                             )
                               ] ;
                    skos:definition "The party that is the source of something (e.g. a message, shipment, etc.)"^^xsd:string ;
                    skos:prefLabel "comes from agent"^^xsd:string ;
                    skos:scopeNote "This is not the inverse of gist:goesToAgent. A message can be to someone. If we made it the inverse the person would be \"from\" the message"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/comesFromPlace
gist:comesFromPlace rdf:type owl:ObjectProperty ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( gist:Address
                                               gist:Place
                                             )
                               ] ;
                    skos:definition "Origin"^^xsd:string ;
                    skos:prefLabel "comes from place"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/conformsTo
gist:conformsTo rdf:type owl:ObjectProperty ;
                rdfs:range gist:Intention ;
                skos:definition "The subject conforms to the Object, e.g. meet an obligation, meet terms of an offer, adhere to a specification"^^xsd:string ;
                skos:prefLabel "conforms to"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/containsGeographically
gist:containsGeographically rdf:type owl:ObjectProperty ;
                            owl:inverseOf gist:isGeographicallyContainedIn ;
                            rdf:type owl:TransitiveProperty ;
                            rdfs:domain gist:Place ;
                            rdfs:range gist:Place ;
                            skos:definition "Relate one place to another place that is contained within the first."^^xsd:string ;
                            skos:prefLabel "contains geographically"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/contributesTo
gist:contributesTo rdf:type owl:ObjectProperty ;
                   skos:definition "The parts of a system contribute to the goal/ function of the whole system"^^xsd:string ;
                   skos:prefLabel "contributes to"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/directs
gist:directs rdf:type owl:ObjectProperty ;
             skos:definition "The set of actuators that a controller can affect"^^xsd:string ;
             skos:prefLabel "directs"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/followsDirectly
gist:followsDirectly rdf:type owl:ObjectProperty ;
                     owl:inverseOf gist:precedesDirectly ;
                     skos:definition "A generic ordering relation indicating that the subject comes immediately after the object."^^xsd:string ;
                     skos:prefLabel "follows directly"^^xsd:string ;
                     skos:scopeNote "It is safest to use this property only when the directness has a semantic correspondence with the world. Only break a direct link by inserting an intermediate item when that change corresponds to a change in the world."^^xsd:string .


###  https://ontologies.semanticarts.com/gist/goesToAgent
gist:goesToAgent rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf gist:hasParticipant ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( gist:Organization
                                            gist:Person
                                          )
                            ] ;
                 skos:definition "The party that is the recipient of something (e.g. a message, shipment, etc.)"^^xsd:string ;
                 skos:prefLabel "goes to agent"^^xsd:string ;
                 skos:scopeNote "This is not the inverse of gist:comesFromAgent. A message can be from someone. If we made it the inverse the person would be \"to\" the message"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/goesToPlace
gist:goesToPlace rdf:type owl:ObjectProperty ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( gist:Address
                                            gist:Place
                                          )
                            ] ;
                 skos:definition "Destination"^^xsd:string ;
                 skos:prefLabel "goes to place"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/governs
gist:governs rdf:type owl:ObjectProperty ;
             owl:inverseOf gist:isGovernedBy ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( gist:Intention
                                         gist:Organization
                                         gist:Person
                                         gist:Template
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( gist:Category
                                        gist:Content
                                        gist:GeoRegion
                                        gist:IntellectualProperty
                                        gist:Intention
                                        gist:Organization
                                        gist:Person
                                        gist:PhysicalIdentifiableItem
                                        gist:PhysicalSubstance
                                      )
                        ] ;
             skos:definition "The subject controls or inhibits the object in some way"^^xsd:string ;
             skos:prefLabel "governs"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasActual
gist:hasActual rdf:type owl:ObjectProperty ;
               rdfs:range gist:TimeInstant ;
               skos:definition "When something did occur, therefore noting an historical event."^^xsd:string ;
               skos:prefLabel "has actual"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasActualEnd
gist:hasActualEnd rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf gist:hasActual ,
                                     gist:hasEnd ;
                  skos:definition "When something did end, therefore noting an historical event."^^xsd:string ;
                  skos:prefLabel "has actual end"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasActualStart
gist:hasActualStart rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf gist:hasActual ,
                                       gist:hasStart ;
                    skos:definition "When something did start, therefore noting an historical event."^^xsd:string ;
                    skos:prefLabel "has actual start"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasAddress
gist:hasAddress rdf:type owl:ObjectProperty ;
                rdfs:range gist:Address ;
                skos:definition "Relates the subject to its physical or virtual address."^^xsd:string ;
                skos:example "The street address of a building; the email address of a person."^^xsd:string ;
                skos:prefLabel "has address"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasAltitude
gist:hasAltitude rdf:type owl:ObjectProperty ;
                 rdfs:domain gist:GeoPoint ;
                 rdfs:range gist:Extent ;
                 skos:definition "Distance above sea level"^^xsd:string ;
                 skos:prefLabel "has altitude"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasBaseUnit
gist:hasBaseUnit rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf gist:hasStandardUnit ;
                 rdfs:domain gist:UnitOfMeasure ;
                 rdfs:range gist:BaseUnit ;
                 skos:definition "Relates a UnitOfMeasure to its BaseUnit.  This indicates what kind of Unit something is."^^xsd:string ;
                 skos:example "Saying that a furlong hasBaseUnit  meter says it is a DistanceUnit."^^xsd:string ;
                 skos:prefLabel "has base unit"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasBiologicalOffspring
gist:hasBiologicalOffspring rdf:type owl:ObjectProperty ;
                            rdfs:domain gist:LivingThing ;
                            rdfs:range gist:LivingThing ;
                            skos:definition "Relates a parent to its biological offspring."^^xsd:string ;
                            skos:prefLabel "has biological offspring"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasBiologicalParent
gist:hasBiologicalParent rdf:type owl:ObjectProperty ;
                         skos:definition "Relates a living thing to its biological parent."^^xsd:string ;
                         skos:prefLabel "has biological parent"^^xsd:string ;
                         gist:rangeIncludes gist:Person .


###  https://ontologies.semanticarts.com/gist/hasBirthDate
gist:hasBirthDate rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf gist:hasStart ;
                  rdfs:domain gist:LivingThing ;
                  rdfs:range gist:TimeInstant ;
                  skos:definition "Date a living thing is or will be born."^^xsd:string ;
                  skos:prefLabel "has birthdate"^^xsd:string ;
                  skos:scopeNote "This property is a subproperty of gist:hasStart, rather than gist:hasActualStart, to acknowledge the fact that the birth may not yet have occurred. In this case, the birthdate is expected although not known."^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasCommunicationAddress
gist:hasCommunicationAddress rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf gist:hasAddress ;
                             rdfs:domain [ rdf:type owl:Class ;
                                           owl:unionOf ( gist:Organization
                                                         gist:Person
                                                       )
                                         ] ;
                             rdfs:range gist:Address ;
                             skos:definition "Relates a Person or Organization to where they can receive messages, including postal addresses, fax numbers, phone numbers, email, web site, etc."^^xsd:string ;
                             skos:prefLabel "has communication address"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasDeathDate
gist:hasDeathDate rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf gist:hasActualEnd ;
                  rdfs:domain gist:LivingThing ;
                  rdfs:range gist:TimeInstant ;
                  skos:definition "Date a living thing died"^^xsd:string ;
                  skos:prefLabel "has death date"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasDenominator
gist:hasDenominator rdf:type owl:ObjectProperty ;
                    skos:definition "Relates a RatioUnit such as meters/second to the denominator Unit (e.g. second)."^^xsd:string ;
                    skos:prefLabel "has denominator"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasDirectPart
gist:hasDirectPart rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf gist:hasPart ;
                   owl:inverseOf gist:isDirectPartOf ;
                   skos:definition "The relationship between a whole and a part where the part has independent existence."^^xsd:string ;
                   skos:prefLabel "has direct part"^^xsd:string ;
                   skos:scopeNote "It is safest to use this property when there is semantic directness inherent in the relationship, rather than choosing appropriate granularity. For example, a spark plug is a direct part of an engine block; there cannot be any intermediate parts.  Beware of making a hasDirectPart assertion and then inserting an intermediate part; this will result in making an asserted triple false even though there was no change in the world."^^xsd:string ,
                                  "No cascading delete."^^xsd:string ,
                                  "Use this property to directly associate a part with the whole. gist:hasPart is the transitive version."^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasDirectSubCategory
gist:hasDirectSubCategory rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf gist:hasSubCategory ;
                          skos:definition "The subject category is a supercategory of the object category. This property defines the direct links in a category hierarchy; no intermediate categories can exist between the direct links."^^xsd:string ;
                          skos:prefLabel "has direct subcategory"^^xsd:string ;
                          skos:scopeNote "Unlike its superproperty gist:hasSubCategory, this property is not transitive. It is essentially the same as the non-transitive skos:narrower, using gist:Category rather than skos:Concept."^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasDirectSubTask
gist:hasDirectSubTask rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf gist:hasSubTask ;
                      owl:inverseOf gist:isDirectSubTaskOf ;
                      rdfs:domain gist:Task ;
                      rdfs:range gist:Task ;
                      skos:definition "Immediate child task"^^xsd:string ;
                      skos:prefLabel "has direct sub task"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasDirectSuperCategory
gist:hasDirectSuperCategory rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf gist:hasSuperCategory ;
                            skos:definition "The subject category is a subcategory of the object category. This property defines the direct links in a category hierarchy; no intermediate categories can exist between the direct links."^^xsd:string ;
                            skos:prefLabel "has direct supercategory"^^xsd:string ;
                            skos:scopeNote "Unlike its superproperty gist:hasSuperCategory, this property is not transitive. It is essentially the same as the non-transitive skos:broader, using gist:Category rather than skos:Concept."^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasEnd
gist:hasEnd rdf:type owl:ObjectProperty ;
            rdfs:range gist:TimeInstant ;
            skos:definition "Connects the subject to its end time."^^xsd:string ;
            skos:prefLabel "has end"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasGiver
gist:hasGiver rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf gist:hasParticipant ;
              owl:propertyDisjointWith gist:hasRecipient ;
              skos:definition "The active party, the one with the obligation or the one initiating the transfer"^^xsd:string ;
              skos:prefLabel "has giver"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasGoal
gist:hasGoal rdf:type owl:ObjectProperty ;
             skos:definition "The reason for doing something"^^xsd:string ;
             skos:prefLabel "has goal"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasIncumbent
gist:hasIncumbent rdf:type owl:ObjectProperty ;
                  skos:definition "What equipment or person is currently in this node.  Note to create a temporal view make a TemporalRelation for this property"^^xsd:string ;
                  skos:prefLabel "has incumbent"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasJurisdictionOver
gist:hasJurisdictionOver rdf:type owl:ObjectProperty ;
                         skos:definition "When laws and contracts are meted out"^^xsd:string ;
                         skos:prefLabel "has jurisdiction over"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasMagnitude
gist:hasMagnitude rdf:type owl:ObjectProperty ;
                  rdfs:range gist:Magnitude ;
                  skos:definition "To have a comparable numeric value. Each magnitude has a unit."^^xsd:string ;
                  skos:prefLabel "has magnitude"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasMember
gist:hasMember rdf:type owl:ObjectProperty ;
               owl:inverseOf gist:isMemberOf ;
               skos:definition "Relates a Collection to its member individuals."^^xsd:string ;
               skos:prefLabel "has member"^^xsd:string ;
               gist:domainIncludes gist:Collection .


###  https://ontologies.semanticarts.com/gist/hasMultiplicand
gist:hasMultiplicand rdf:type owl:ObjectProperty ;
                     skos:definition "Relates a ProductUnit such as square mile to the second of two units multiplied together (e.g. mile)."^^xsd:string ;
                     skos:prefLabel "has multiplicand"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasMultiplier
gist:hasMultiplier rdf:type owl:ObjectProperty ;
                   skos:definition "Relates a ProductUnit such as square mile to the first of two units multiplied together (e.g. mile)"^^xsd:string ;
                   skos:prefLabel "has multiplier"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasNavigationalChild
gist:hasNavigationalChild rdf:type owl:ObjectProperty ;
                          owl:inverseOf gist:hasNavigationalParent ;
                          skos:definition "A property that relates a parent category to a child category in an informal (e.g., faceted) hierarchy."^^xsd:string ;
                          skos:example "Refrigerator handles are not refrigerators, but it may be useful to represent their relationship hierarchically for a faceted UI filter."^^xsd:string ;
                          skos:prefLabel "has navigational child"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasNavigationalParent
gist:hasNavigationalParent rdf:type owl:ObjectProperty ;
                           skos:definition "A property that relates a child category to a parent category in an informal (e.g., faceted) hierarchy."^^xsd:string ;
                           skos:example "Refrigerator handles are not refrigerators, but it may be useful to represent their relationship hierarchically for a faceted UI filter."^^xsd:string ;
                           skos:prefLabel "has navigational parent"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasNumerator
gist:hasNumerator rdf:type owl:ObjectProperty ;
                  skos:definition "Relates a RatioUnit such as meter(s)/second to the numerator Unit (e.g. meter)."^^xsd:string ;
                  skos:prefLabel "has numerator"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasOffsetToUniversal
gist:hasOffsetToUniversal rdf:type owl:ObjectProperty ;
                          rdfs:domain gist:TimeZone ;
                          rdfs:range gist:Duration ;
                          skos:definition "How many hours the time zone is off GMT"^^xsd:string ;
                          skos:prefLabel "has offset to universal"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasPart
gist:hasPart rdf:type owl:ObjectProperty ;
             owl:inverseOf gist:isPartOf ;
             rdf:type owl:TransitiveProperty ;
             skos:definition "The transitive version of gist:hasDirectPart"^^xsd:string ;
             skos:prefLabel "has part"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasParticipant
gist:hasParticipant rdf:type owl:ObjectProperty ;
                    skos:definition "Relates something (e.g. an agreement) to things that play a role, or take part or are otherwise involved in some way."^^xsd:string ;
                    skos:example "An event of transferring money has a participating account that receives the money."^^xsd:string ;
                    skos:prefLabel "has participant"^^xsd:string ;
                    skos:scopeNote "The thing with participants will often be an agreement, event or obligation."^^xsd:string ,
                                   "This is intended as an abstract property. Only its subproperties will be directly used."^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasParty
gist:hasParty rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf gist:hasParticipant ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( gist:Organization
                                         gist:Person
                                       )
                         ] ;
              skos:definition "The people or organizations participating in an event, agreement or obligation"^^xsd:string ;
              skos:example "For loan agreements, one might create hasLender and hasBorrower as subproperties of hasParty."^^xsd:string ;
              skos:prefLabel "has party"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasPhysicalLocation
gist:hasPhysicalLocation rdf:type owl:ObjectProperty ,
                                  owl:TransitiveProperty ;
                         rdfs:range gist:Place ;
                         skos:definition "Where something is located"^^xsd:string ;
                         skos:prefLabel "has physical location"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasPlanned
gist:hasPlanned rdf:type owl:ObjectProperty ;
                rdfs:range gist:TimeInstant ;
                skos:definition "Dates that were in the future at the time they were made."^^xsd:string ;
                skos:prefLabel "has planned"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasPlannedEnd
gist:hasPlannedEnd rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf gist:hasEnd ,
                                      gist:hasPlanned ;
                   rdfs:range gist:TimeInstant ;
                   skos:definition "A date/time that was at least at some point in time in the future. It may be in the past now, but when we planned it, it was in the future."^^xsd:string ;
                   skos:prefLabel "has planned end"^^xsd:string ;
                   skos:scopeNote "Most frequently apples to Event(s) and Offer(s). E.g. a conference or sale offer."^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasPlannedStart
gist:hasPlannedStart rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf gist:hasPlanned ,
                                        gist:hasStart ;
                     rdfs:range gist:TimeInstant ;
                     skos:definition "A date/time that was at least at some point in time in the future. It may be in the past now, but when we planned it, it was in the future."^^xsd:string ;
                     skos:prefLabel "has planned start"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasPrecision
gist:hasPrecision rdf:type owl:ObjectProperty ;
                  rdfs:range gist:Magnitude ;
                  skos:definition "Links a Magnitude to the degree of accuracy of the numeric value.   This allows for fuzzy numbers.  All magnitudes have a precision.  Usually we don't record them.  When we do this, it will be a value whose extent covers 2 standard deviations around the stated magnitude"^^xsd:string ;
                  skos:example "Temperature precise to tenth of a degree C; TimeInstant precise to 24 hours."^^xsd:string ;
                  skos:prefLabel "has precision"^^xsd:string ;
                  skos:scopeNote "Most frequently apples to Magnitude(s) and TimeInstant. Could also apply to a measurement."^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasRecipient
gist:hasRecipient rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf gist:hasParticipant ;
                  skos:definition "The recipient"^^xsd:string ;
                  skos:prefLabel "has recipient"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasStandardUnit
gist:hasStandardUnit rdf:type owl:ObjectProperty ;
                     rdfs:domain gist:UnitOfMeasure ;
                     rdfs:range gist:CoherentUnit ;
                     skos:definition "For a complex unit refers to a unit that has all the component parts in SI"^^xsd:string ;
                     skos:prefLabel "has standard unit"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasStart
gist:hasStart rdf:type owl:ObjectProperty ;
              rdfs:range gist:TimeInstant ;
              skos:definition "Connects the subject to its start time."^^xsd:string ;
              skos:prefLabel "has start"^^xsd:string .


###  https://ontologies.semanticarts.com/gist/hasSubCategory
gist:hasSubCategory rdf:type owl:ObjectProperty ;
                    owl:inverseOf gist:hasSuperCategory ;
                    skos:definition "The subject category is inclusive of, or broader than, the object category. Everything categorized by the subcategory can be inferred to be categorized by the supercategory."^^xsd:string ;
                    skos:prefLabel "has subcategory"^^xsd:string ;
