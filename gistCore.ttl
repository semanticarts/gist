gist:TemperatureUnit
	a owl:Class ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			gist:SimpleUnitOfMeasure
			[
				a owl:Restriction ;
				owl:onProperty gist:hasBaseUnit ;
				owl:hasValue gist:_BaseUnit__kelvin ;
